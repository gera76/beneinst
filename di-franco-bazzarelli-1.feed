	<entry>
		<title>La visione dei tuoi occhi</title>
		<link rel="alternate" type="text/html" href="https://www.beneinst.it/di-franco-bazzarelli.html?view=article&amp;id=73&amp;catid=17"/>
		<published>2023-03-16T15:50:37+00:00</published>
